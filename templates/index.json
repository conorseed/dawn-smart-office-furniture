    "a906c273-a8eb-416e-ade7-04155872f8f7": {
      "type": "slideshow",
      "blocks": {
        "facc43d7-900f-45b5-ac4e-c1f570dbdc7f": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/ABC9559.jpg",
            "heading": "Christmas hours",
            "heading_size": "h0",
            "subheading": "Our shop will be closed on December 25th 2023 and we will re-open again on 8 January 2024 with reduced hours from the 8th January to the 12th January(8:00am till 3:00pm)Any order placed online during these dates will not be looked at until we re-open. Merry Christmas from the team here at Footwear and Apparel!",
            "button_label": "",
            "link": "",
            "button_style_secondary": false,
            "box_align": "middle-right",
            "show_text_box": true,
            "text_alignment": "center",
            "image_overlay_opacity": 0,
            "color_scheme": "accent-2",
            "text_alignment_mobile": "center"
          }
        },
        "template--14388672135243__a906c273-a8eb-416e-ade7-04155872f8f7-slide-1": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/home-screen.jpg",
            "heading": "Digital Transfer & Embroidery",
            "heading_size": "h2",
            "subheading": "At Footwear and Apparel, we take pride in offering top-notch digital transfer and embroidery services. Whether you want to promote your brand or create personalised apparel, our skilled team can add your logo or design to any garment. With our meticulous attention to detail and high-quality materials, we ensure that your message stands out in style. Don't miss out on this opportunity to make a lasting impression.",
            "button_label": "Find out more",
            "link": "shopify:\/\/pages\/contact-us",
            "button_style_secondary": false,
            "box_align": "middle-right",
            "show_text_box": true,
            "text_alignment": "left",
            "image_overlay_opacity": 0,
            "color_scheme": "accent-2",
            "text_alignment_mobile": "center"
          }
        },
        "template--14388672135243__a906c273-a8eb-416e-ade7-04155872f8f7-slide-2": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/home-sizing.jpg",
            "heading": "Sizing Charts",
            "heading_size": "h2",
            "subheading": "Ensuring the perfect fit for your footwear and apparel is paramount to us. That's why we source our sizing charts directly from our suppliers, guaranteeing accurate measurements for all our products. Our meticulous approach ensures that you can confidently choose the right size and experience unparalleled comfort every time you shop with us. Say farewell to guesswork and make informed decisions with our comprehensive and detailed sizing charts.",
            "button_label": "Find your ideal fit",
            "link": "#",
            "button_style_secondary": false,
            "box_align": "middle-right",
            "show_text_box": true,
            "text_alignment": "left",
            "image_overlay_opacity": 0,
            "color_scheme": "accent-2",
            "text_alignment_mobile": "center"
          }
        },
        "61ed6fc5-1c7e-42cf-98dc-66406f546419": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/home-about_0c9fa9fd-2a3f-40bc-91a0-721dd70d9d1d.jpg",
            "heading": "Meet the team",
            "heading_size": "h2",
            "subheading": "We're proud to introduce you to our incredible team. Behind every successful company, there are dedicated individuals who work tirelessly to bring you the best products and service. Each member of our team plays a crucial role in delivering a seamless experience for you. Meet our exceptional team members and learn more about the people who make Footwear and Apparel thrive.",
            "button_label": "Meet the team",
            "link": "shopify:\/\/pages\/about-us",
            "button_style_secondary": false,
            "box_align": "middle-right",
            "show_text_box": true,
            "text_alignment": "left",
            "image_overlay_opacity": 0,
            "color_scheme": "accent-2",
            "text_alignment_mobile": "center"
          }
        }
      },
